public class Solution {
    public string ToLowerCase(string str){
        string newstr = "";
        for (int i = 0; i < str.Length; i++)
        {
            if (str[i] >= 65 && str[i] <= 90){
                newstr += (char)(str[i] + 32);
            }
            else{
                newstr += (char)(str[i]);
            }
        }
        return newstr;
    }
}
