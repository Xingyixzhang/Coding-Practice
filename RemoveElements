public class Solution {
    public int RemoveElement(int[] nums, int val) {
        int x = 0;
        if (nums.Length == 0) return 0;
        else{
            for (int j = 0; j < nums.Length; j++)
            {
                if (nums[j] != val){
                    nums[x] = nums[j];
                    x += 1;
                }
            }
        }
        return x;
    }
}
